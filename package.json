{
  "name": "slabbot",
  "version": "0.3.0",
  "description": "an experimental discord bot with avant-garde features (that no one ever asked for).",
  "main": "index.js",
  "scripts": {
    "test": "eslint commands listeners *.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AndyThePie/slabbot.git"
  },
  "author": "AndyThePie",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AndyThePie/slabbot/issues"
  },
  "homepage": "https://github.com/AndyThePie/slabbot#readme",
  "dependencies": {
    "discord-akairo": "^8.1.0",
    "discord.js": "^12.5.1",
    "mongoose": "^5.12.1",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "eslint": "7.30.0",
    "eslint-config-xo": "0.37.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-unicorn": "34.0.1"
  },
  "eslintConfig": {
    "env": {
      "commonjs": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "xo"
    ],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
      "capitalized-comments": 0,
      "no-warning-comments": 0,
      "radix": 0,
      "quotes": [
        1,
        "double",
        {
          "avoidEscape": true,
          "allowTemplateLiterals": true
        }
      ],
      "comma-dangle": [
        "error",
        "never"
      ]
    }
  },
  "renovate": {
    "extends": [
      "config:base"
    ],
    "labels": [
      "dependencies"
    ],
    "packageRules": [
      {
        "matchUpdateTypes": [
          "minor",
          "patch",
          "pin",
          "digest"
        ],
        "automerge": true,
        "stabilityDays": 3
      }
    ]
  }
}
