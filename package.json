{
  "name": "slabbot",
  "version": "0.5.0-next",
  "description": "another discord bot with features nobody asked for",
  "private": "true",
  "type": "module",
  "scripts": {
    "lint": "eslint src --ext .ts",
    "build": "tsc",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "start": "node dist/index.js",
    "dev": "nodemon dist/index.js | pino-pretty -i pid,hostname -t yyyy-mm-dd HH:MM:ss",
    "deploy": "node dist/manual/deploy-commands.js",
    "speak": "node dist/manual/speak.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/12beesinatrenchcoat/slabbot.git"
  },
  "author": "12beesinatrenchcoat <12beesinatrenchcoat+removethisbitifyouarenotspam@protonmail.com>",
  "license": "MIT",
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "@barelyhuman/tocolor": "^0.0.9",
    "@discordjs/builders": "^0.15.0",
    "@discordjs/rest": "^0.5.0",
    "dayjs": "^1.11.3",
    "discord-api-types": "^0.36.2",
    "discord.js": "^13.9.0",
    "dotenv": "^16.0.1",
    "git-last-commit": "^1.0.1",
    "mongoose": "^6.4.4",
    "node-fetch": "^3.2.8",
    "pino": "^8.1.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0"
  },
  "devDependencies": {
		"@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.20.0",
    "eslint-config-xo": "^0.41.0",
    "nodemon": "^2.0.19",
    "pino-pretty": "^8.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
		"typescript": "^4.7.4"
  }
}
