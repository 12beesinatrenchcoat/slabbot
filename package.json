{
  "name": "slabbot",
  "version": "0.5.0-next",
  "description": "another discord bot with features nobody asked for",
  "private": "true",
  "type": "module",
  "scripts": {
    "lint": "eslint src --ext .ts",
    "build": "tsc",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "start": "node dist/index.js | pino-pretty -i pid,hostname -t 'yyyy-mm-dd HH:MM:ss' -L info",
    "dev": "nodemon dist/index.js | pino-pretty -i pid,hostname -t HH:MM:ss.l",
    "mongo": "mongod --dbpath ./db",
    "deploy": "node dist/manual/deploy-commands.js",
    "speak": "node dist/manual/speak.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/12beesinatrenchcoat/slabbot.git"
  },
  "author": "Andy Chan (12beesinatrenchcoat) <andy@12beesinatrenchco.at>",
  "license": "MIT",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "dependencies": {
    "@discordjs/rest": "^1.7.1",
    "colord": "^2.9.3",
    "dayjs": "^1.11.8",
    "discord-api-types": "^0.37.43",
    "discord.js": "^14.11.0",
    "dotenv": "^16.1.4",
    "mongoose": "^7.3.0",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.1",
    "pino": "^8.14.1",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-xo": "^0.43.1",
    "nodemon": "^2.0.22",
    "pino-pretty": "^10.0.0",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}
